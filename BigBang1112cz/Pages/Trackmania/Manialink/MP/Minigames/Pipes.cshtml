<script><![CDATA[
		#Include "MathLib" as Math
		#Include "AnimLib" as Anim
		#Include "TextLib" as TXT

		declare Integer score;
		declare Integer turns;
		declare Real time;
		declare Integer bonus;
		declare Integer levelWeight;

		declare Integer levelState; //0=none 1=random 2=specific 3=playing
		declare Text levelDifficulty; //easy,hard

		declare Boolean level1;
		declare Boolean level2;

		declare Boolean checkingConnection;

		Void GenerateRandomLevel() {
			foreach(pipe in ((Page.GetFirstChild("Pipes") as CMlFrame).Controls)) {
				pipe.RelativeRotation = Math::NearestReal(Math::Rand(1,4) * 90);

				if(Math::Rand(0,1) == 0) {
					pipe.DataAttributeSet("type","Turn");
				}
				else {
					pipe.DataAttributeSet("type","Straight");
				}
			}
			foreach(pipe in ((Page.GetFirstChild("Pipes2") as CMlFrame).Controls)) {
				pipe.RelativeRotation = Math::NearestReal(Math::Rand(1,4) * 90);

				if(Math::Rand(0,1) == 0) {
					pipe.DataAttributeSet("type","Turn");
				}
				else {
					pipe.DataAttributeSet("type","Straight");
				}
			}
		}

		Void RefreshPipes() {
			foreach(pipe in ((Page.GetFirstChild("Pipes") as CMlFrame).Controls)) {
				if((pipe as CMlFrame).DataAttributeGet("type") == "Straight") {
					((pipe as CMlFrame).Controls[1] as CMlQuad).ChangeImageUrl("https://@(Request.Host)/trackmania/manialink/mp/minigames/pipes/pipe_straight.png");

					((pipe as CMlFrame).Controls[3] as CMlQuad).DataAttributeSet("connection","1");
					((pipe as CMlFrame).Controls[4] as CMlQuad).DataAttributeSet("connection","0");
					((pipe as CMlFrame).Controls[5] as CMlQuad).DataAttributeSet("connection","1");
					((pipe as CMlFrame).Controls[6] as CMlQuad).DataAttributeSet("connection","0");
				}
				else {
					((pipe as CMlFrame).Controls[1] as CMlQuad).ChangeImageUrl("https://@(Request.Host)/trackmania/manialink/mp/minigames/pipes/pipe_turn.png");

					((pipe as CMlFrame).Controls[3] as CMlQuad).DataAttributeSet("connection","1");
